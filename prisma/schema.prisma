generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Feature {
    id          String   @id @default(uuid()) @db.Uuid
    title       String
    description String
    image       Bytes?
    createdAt   DateTime @default(now())
}

model Step {
    id   String @id @default(uuid()) @db.Uuid
    step String
}
